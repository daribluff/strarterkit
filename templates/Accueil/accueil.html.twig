    <META HTTP-EQUIV="content-language" CONTENT="fr">
    <META HTTP-EQUIV="Reply-to" CONTENT="postmaster@myliveleisure.com">

    <link rel="icon" type="image/png" href="{{ asset('/web/css/images/mlllogo.png') }}" />

    <meta name="viewport" content="width=512"/>

    <title>Undecesquatre - Agenda partagé social</title>

    {% block stylesheets %}

        <link rel="stylesheet" href="{{ asset('/web/css/accueil.css') }}" type="text/css" />

    {% endblock %}

